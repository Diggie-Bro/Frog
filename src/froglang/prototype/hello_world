import io

func foo(name, count, yes) {
    if (yes == "yes") {
        run print("yes!")
    }
    elif (yes == "no") {
        run print("no...")
    }
    for (1...count) {
        run print("hello @name")
    }
}

main {
    var name = "frog"
    name = "froglang"
    const mem = "100kb"
    run foo(name, 3, "yes")
}